# Required
PRIVATE_KEY=0xYOUR_DEPLOYER_PRIVATE_KEY
RPC_URL=https://your-rpc-endpoint

# Optional role targets (default: deployer)
# OWNER=0xYourOwnerAddress
# OPERATOR=0xYourOperatorAddress

# Optional auto-accept keys for 2-step role handoff
# OWNER_PRIVATE_KEY=0xYourOwnerPrivateKey
# OPERATOR_PRIVATE_KEY=0xYourOperatorPrivateKey

# Optional token config
# If unset or 0x000...000, script deploys MockUSDC
USDC_ADDRESS=0x0000000000000000000000000000000000000000

# Optional pool config
ANNUAL_CAP_USDC6=50000000000000
CLAIM_WINDOW_SEC=0
INITIAL_POOL_FUND_USDC6=5000000000000

# Optional payout engine config
SCALE_WAD=1000000000000000000
CORRIDOR_DEDUCT_USDC6=0
EVENT_CAP_USDC6=0
